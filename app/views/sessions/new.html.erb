    <div class="row">
      <div class="col-md-4 col-md-offset-4">
        <%= form_for(:session, url: login_path) do |f| %>
        <form>
          <div class="form-group">
            <%= f.label :email  %>
            <%= f.text_field :email, type: 'email', class: 'form-control', id: 'email', placeholder: 'Email' %>
          </div>
          <div class="form-group">
            <%= f.label :password %>
            <%= f.text_field :password, type: 'password', class: 'form-control', id: 'password', placeholder: 'Password' %>
          </div>
          <%= f.submit 'Login', class: 'btn btn-default'%>
        </form>
        <% end %>
      </div>
    </div>
